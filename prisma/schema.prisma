generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model note {
  id         String  @id
  noteId     String? @default("no data")
  content    String?
  tag        String? @default("[]")
  attributes String? @default("{}")
  relations  String? @default("[]")
}

model relation {
  id       String  @id
  name     String?
  relation String? @default("{}")
}

model tag {
  id Int @id @default(autoincrement())
}
